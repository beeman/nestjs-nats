{
  "name": "nestjs-nats",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:master": "ng serve master",
    "start:minion": "ng serve minion --configuration development",
    "start:master:prod": "node dist/apps/master/main.js",
    "start:minion:prod": "node dist/apps/minion/main.js",
    "build": "ng build master --prod && ng build minion --prod",
    "docker:build": "docker build -t beeman/nestjs-nats-master:latest .",
    "docker:push": "docker push beeman/nestjs-nats-master:latest",
    "docker:run": "docker-compose up",
    "test": "ng test",
    "lint": "nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "@nestjs/microservices": "^6.5.2",
    "@nestjs/platform-express": "^6.2.4",
    "cloudevents-sdk": "^0.2.3",
    "nats": "^1.3.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@angular/cli": "8.0.0",
    "@nestjs/schematics": "^6.3.0",
    "@nestjs/testing": "^6.2.4",
    "@nrwl/jest": "8.2.0",
    "@nrwl/nest": "8.2.0",
    "@nrwl/node": "8.2.0",
    "@nrwl/workspace": "8.2.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "dotenv": "6.2.0",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
